[core]
as_active_plugins = core;ccp;move;resize;place;decoration;text;grid;png;regex;gnomecompat;dbus;workarounds;imgjpeg;session;mousepoll;svg;commands;resizeinfo;wall;snap;animation;fade;expo;ezoom;scale;scaleaddon;switcher;
as_audible_bell = false
as_edge_delay = 450
as_cursor_theme = DMZ-White
as_autoraise = false
as_autoraise_delay = 500
as_maximize_window_key = Disabled
as_show_desktop_key = <Super>d
as_toggle_window_maximized_key = <Alt>F10
as_toggle_window_shaded_key = Disabled
s0_hsize = 2
s0_vsize = 2

[grid]
as_put_center_key = <Super>k
as_put_left_key = <Super>j
as_put_right_key = <Super>l
as_put_top_key = <Super>i
as_put_bottom_key = <Super>comma
as_put_topleft_key = <Super>u
as_put_topright_key = <Super>o
as_put_bottomleft_key = <Super>m
as_put_bottomright_key = <Super>period

[ezoom]
as_zoom_in_key = <Mod4>z
as_zoom_out_key = <Mod4>x

[decoration]
as_shadow_radius = 9.000000
as_command = gtk-window-decorator --replace

[group]
s0_change_animation_time = 0.100000
s0_thumb_size = 160
s0_tabbar_font_size = 8
s0_glow_size = 30

[wall]
as_left_key = <Super>Left
as_right_key = <Super>Right
as_up_key = <Super>Up
as_down_key = <Super>Down
as_left_window_key = <Shift><Super>Left
as_right_window_key = <Shift><Super>Right
as_up_window_key = <Shift><Super>Up
as_down_window_key = <Shift><Super>Down
as_flip_left_edge = 
as_flip_right_edge = 
as_flip_up_edge = 
as_flip_down_edge = 

[put]
as_put_left_key = Disabled
as_put_right_key = Disabled
as_put_top_key = Disabled
as_put_bottom_key = Disabled
as_put_topleft_key = Disabled
as_put_topright_key = Disabled
as_put_bottomleft_key = Disabled
as_put_bottomright_key = Disabled

[gnomecompat]
as_run_command_terminal_key = <Super>q

[winrules]
s0_maximize_match = title=

[obs]
s0_opacity_matches = ;
s0_opacity_values = 0;

[fade]
s0_fullscreen_visual_bell = true

[switcher]
as_next_key = Disabled
as_next_no_popup_key = <Alt>Tab
s0_zoom = 0.000000

[commands]
as_command0 = wmctrl -a vimperator
as_command1 = wmctrl -a gvim
as_command2 = wmctrl -a fran@fran
as_command3 = wmctrl -a gmail
as_run_command0_key = <Super>0
as_run_command1_key = <Super>9
as_run_command2_key = <Super>8
as_run_command3_key = <Super>1

